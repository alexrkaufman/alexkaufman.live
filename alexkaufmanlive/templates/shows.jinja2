{% extends "base.jinja2" %}

{% block main %}
    <h1> Upcoming Shows </h1>
    {% for show in upcoming_shows %}
        <a href="{{ show.slug }}">
            <h2>{{ show['title'] }}</h2>
        </a>
    {% endfor %}
    <h1> Past Shows </h1>
    {% for show in past_shows %}
        <a href="{{ show.slug }}">
            <h2>{{ show['title'] }}</h2>
        </a>
    {% endfor %}
{% endblock %}
